{
  "name": "project-movie-back-end",
  "version": "1.0.0",
  "description": "zid-be-project-movie-01-qualified",
  "main": "src/app.js",
  "scripts": {
    "docker:build": "docker image build . -t thinkful-ed/zid-be-project-movie-01-qualified",
    "docker:run": "docker run --rm -it -p 5001:5001 thinkful-ed/zid-be-project-movie-01-qualified",
    "docker:stop": "docker stop $(docker ps -q)",
    "docker:test": "docker run -t  thinkful-ed/zid-be-project-movie-01-qualified npm test",
    "knex": "knex",
    "migrate": "knex migrate:latest",
    "migrate:dev": "cross-env NODE_ENV=development knex migrate:latest",
    "migrate:test": "cross-env NODE_ENV=test knex migrate:latest",
    "migrate:prod": "cross-env NODE_ENV=production knex migrate:latest",
    "reset": "npm run rollback && npm run migrate && npm run seed",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:dev": "cross-env NODE_ENV=development knex seed:run",
    "seed:test": "cross-env NODE_ENV=test knex migrate:latest && cross-env NODE_ENV=test knex seed:run",
    "seed:prod": "cross-env NODE_ENV=production knex seed:run",
    "start": "node dist/server.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start:solution": "docker-compose up --build",
    "test": "jest --config jest.config.ts",
    "test:solution": "docker-compose build && docker-compose run --rm api npm test && docker-compose down",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "db:reset:dev": "cross-env NODE_ENV=development knex migrate:rollback && cross-env NODE_ENV=development knex migrate:latest && cross-env NODE_ENV=development knex seed:run",
    "db:reset:test": "cross-env NODE_ENV=test knex migrate:rollback --all && cross-env NODE_ENV=test knex migrate:latest && cross-env NODE_ENV=test knex seed:run",
    "db:reset:prod": "cross-env NODE_ENV=production knex migrate:rollback && cross-env NODE_ENV=production knex migrate:latest && cross-env NODE_ENV=production knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "objection": "^3.1.5",
    "pg": "^8.4.2",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^2.0.6",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
